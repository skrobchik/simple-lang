read m
n = 1
minusOne = 0 - 1
while m - n 
    isPrime = minusOne
    if n - 2
      isPrime = 1
    end
    i = 2
    b = i * i
    while n - b
        r = n
        while r - i
            r = r - i
        end
        if minusOne * r
            isPrime = minusOne
        end
        i = i + 1
        b = i * i
    end
    if isPrime
        print n
    end
    n = n + 1
end